<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <Script>

        //DATOS
        calificacion = prompt("ingrese Calificacion (Valores 0-10)")

        //Proceso
        letracal = "";

        if (calificacion == 10) {
            letracal = "A";
            Alert("CALIFICACION:" + letracal);
        } else if (calificacion == 9) {
            letracal = "B";
            alert("CALIFICACION:" + letracal);
        } else if (calificacion == 8) {
            letracal = "C";
            alert("CALIFICACION:" + letracal);
        } else if (calificacion >= 6 && calificacion <= 7) {
            letracal = "D";
            alert("CALIFICACION:" + letracal);
        } else if (calificacion >= 0 && calificacion <= 5) {
            letracal = "F";
            alert("CALIFICACION:" + letracal);
        } else {
            alert("ingresar Numero Valido");
        }
        document.write("TU CALIFICACION ES:  " + letracal);



    </Script>

</body>
